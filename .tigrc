bind main G !@hub browse -- commit/%(commit)
bind diff G !@hub browse -- commit/%(commit)
set ignore-case = smart-case

set main-view-date = custom
set main-view-date-format = "%Y/%m/%d %H:%M"
set blame-view-date = custom
set blame-view-date-format = "%y/%m/%d"
set diff-highlight = /opt/homebrew/opt/git/share/git-core/contrib/diff-highlight/diff-highlight
